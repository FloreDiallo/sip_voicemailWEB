<?xml version="1.0" encoding="ISO-8859-1"?>
<web-app xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
	version="2.5">
	<servlet>
		<servlet-name>Hello</servlet-name>
		<servlet-class>se.ltu.M7017E.lab3.HelloServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Hello</servlet-name>
		<url-pattern>/hello/*</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>Message</servlet-name>
		<servlet-class>se.ltu.M7017E.lab3.MessageServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Message</servlet-name>
		<url-pattern>/message/*</url-pattern>
	</servlet-mapping>
		
	<servlet>
		<servlet-name>MessageList</servlet-name>
		<servlet-class>se.ltu.M7017E.lab3.MessageListServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>MessageList</servlet-name>
		<url-pattern>/message_list/*</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>GetMessage</servlet-name>
		<servlet-class>se.ltu.M7017E.lab3.GetMessageServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>GetMessage</servlet-name>
		<url-pattern>/getmessage/*</url-pattern>
	</servlet-mapping>
</web-app>